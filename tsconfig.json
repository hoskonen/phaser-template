{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "dom",
      "scripthost"
    ],
    "outDir": "./build/"
  },
  "include": [
    "lib/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
