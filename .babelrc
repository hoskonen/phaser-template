{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false
      },
      "@babel/preset-typescript"
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@assets": "./src/assets",
          "@scenes": "./src/scenes"
        }
      }
    ]
  ]
}
